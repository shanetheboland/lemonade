{
  "name": "lemonade",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-dev": "NODE_ENV=development nodemon index.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "lint": "xo"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "mongoose": "^4.4.4",
    "morgan": "~1.6.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-config-xo-react": "^0.5.0",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "react-transform-hmr": "^1.0.2",
    "redux-logger": "^2.6.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1",
    "xo": "^0.12.1",
    "xo-loader": "^0.5.0"
  },
  "xo": {
    "esnext": true,
    "plugin": "react",
    "extends": "xo-react",
    "ignores": [
      "public/javascripts/**"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "babel/object-curly-spacing": [
        2,
        "always"
      ],
      "new-cap": 0,
      "quote-props": 0,
      "prefer-const": 0,
      "no-unused-vars": 0,
      "react/jsx-no-bind": 0
    }
  }
}
